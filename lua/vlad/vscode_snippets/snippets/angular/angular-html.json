{
  "@for": {
    "prefix": "ng-@for",
    "body": [
      "@for ($1 of $2; track $1) {",
      "  $3",
      "}"
    ],
    "description": "Angular @for"
  },
  "@for-empty": {
    "prefix": "ng-@for-empty",
    "body": [
      "@for ($1 of $2; track $1) {",
      "  $3",
      "}",
      "@empty {",
      "  $4",
      "}"
    ],
    "description": "Angular @for with empty"
  },
  "@switch": {
    "prefix": "ng-@switch",
    "body": [
      "@switch ($1) {",
      "  @case ($2) {",
      "    $3",
      "  }",
      "  @default {",
      "    $4",
      "  }",
      "}"
    ],
    "description": "Angular @switch"
  },
  "@if": {
    "prefix": "ng-@if",
    "body": [
      "@if ($1) {",
      "  $2",
      "}"
    ],
    "description": "Angular @if"
  },
  "@ifelse": {
    "prefix": "ng-@ifelse",
    "body": [
      "@if ($1) {",
      "  $2",
      "} @else {",
      "  $3",
      "}"
    ],
    "description": "Angular @if else"
  }
}
