{
    "Handlebars comment": {
        "prefix": "hbs-comment",
        "body": "{{! $1 }}",
        "description": "Handlebars comment"
    },
    "Handlebars Output Value": {
        "prefix": "hbsout",
        "body": "{{ $1 }}",
        "description": "Handlebars outputs value"
    },
    "Handlebars Output Escaped": {
        "prefix": "hbsesc",
        "body": "{{{ $1 }}}",
        "description": "Handlebars outputs value"
    },
    "Handlebars If Statement": {
        "prefix": "hbsif",
        "body": "{{#if $1}} $2 {{/if}}",
        "description": "Handlebars if statement"
    },
    "Handlebars Else Statement": {
        "prefix": "hbselse",
        "body": "{{else}} $1",
        "description": "Handlebars else statement"
    },
    "Handlebars Else If Statement": {
        "prefix": "hbselif",
        "body": "{{else if $1}} $2",
        "description": "Handlebars else if statement"
    },
    "Handlebars Unless Statement": {
        "prefix": "hbsunless",
        "body": "{{#unless $1}} $2 {{/unless}}",
        "description": "Handlebars unless statement"
    },
    "Handlebars Each": {
        "prefix": "hbseach",
        "body": "{{#each $1}} $2 {{/each}}",
        "description": "Handlebars each loop"
    },
    "Handlebars Each As": {
        "prefix": "hbseachas",
        "body": "{{#each $1 as |$2|}} $3 {{/each}}",
        "description": "Handlebars each loop with alias"
    },
    "Handlebars With": {
        "prefix": "hbswith",
        "body": "{{#with $1 as |$2|}} $3 {{/with}}",
        "description": "Handlebars with block"
    },
    "Handlebars Partial": {
        "prefix": "hbspartial",
        "body": "{{> $1 }}",
        "description": "Handlebars partial"
    },
    "Handlebars Block Helper": {
        "prefix": "hbsblock",
        "body": "{{# $1 }} $2 {{/ $1 }}",
        "description": "Handlebars block helper"
    },
    "Handlebars Inline Helper": {
        "prefix": "hbsinline",
        "body": "{{ $1 $2 }}",
        "description": "Handlebars inline helper"
    }
}

